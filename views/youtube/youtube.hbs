    <script>
      
      window.onload = function() {
        var dataset = [];
        var chart;
        $.getJSON("http://54.246.43.67/end", function(json) {  
          
          dataset = json

          
          chart = new CanvasJS.Chart("positiveChart",{
            title:{
              text: dataset.postivePercent+"%",
              verticalAlign: "center",
              dockInsidePlotArea: true,
              fontSize: 30,
            },

            data: [{
              type: "doughnut",
          		innerRadius: 50,
              startAngle: 270,
              dataPoints : [
                { y: dataset.postivePercent, color:"#56efc3" },
                { y: 100-dataset.postivePercent, color:"#ffffff" },
              ],
            }]
          });
          chart.render();

          chart1 = new CanvasJS.Chart("negativeChart",{
            title:{
              text: dataset.negativePercent+"%",
              verticalAlign: "center",
              dockInsidePlotArea: true,
              fontSize: 30,
            },

            data: [{
              	type: "doughnut",
          		  innerRadius: 50,
                startAngle: 270,
              	dataPoints : [
					        { y: dataset.negativePercent, color:"#ef8685" },
					        { y: 100-dataset.negativePercent, color:"#ffffff" },
                ],
            }]
          });
          chart1.render();

          chart2 = new CanvasJS.Chart("neutralChart",{
            title:{
              text: dataset.neutralPercent+"%",
              verticalAlign: "center",
              dockInsidePlotArea: true,
              fontSize: 30,
            },

            data: [{
              	type: "doughnut",
          		  innerRadius: 50,
                startAngle: 270,
              	dataPoints : [
                { y: dataset.neutralPercent, color:"#b3b3b3" },
                { y: 100-dataset.neutralPercent, color:"#f4f4f4" }
              ],
            }]
          });
          chart2.render();

          chart3 = new CanvasJS.Chart("overallChart",{
            title:{
              text: dataset.overallPercent+"%",
              verticalAlign: "center",
              dockInsidePlotArea: true,
              fontSize: 30,
            },

            data: [{
              	type: "doughnut",
          		  innerRadius: 50,
                startAngle: 270,
              	dataPoints : [
					      { y: dataset.overallPercent, color:"#f4f4f4" },
                { y: 100-dataset.overallPercent, color:"#ffffff" }
              ],
            }]
          });
          chart3.render();
      });
      }
        

     
    </script>
